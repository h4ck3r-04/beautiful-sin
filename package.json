{
  "name": "beautiful-sin",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "web-check-api-dev": "cd api/web-check && go run main.go",
    "web-check-api-build": "cd api/web-check && go build -o webcheck",
    "web-check-api-prod": "./api/web-check/main",
    "fastapi-build": "python3 -m pip install -r requirements.txt",
    "fsqli-fastapi-dev": "python3 -m uvicorn api.fsqli.index:app --reload",
    "fsqli-fastapi-prod": "uvicorn api.fsqli.index:app",
    "next-dev": "next dev",
    "next-build": "next build",
    "next-prod": "next start",
    "dev": "concurrently \"npm run next-dev\" \"npm run fsqli-fastapi-dev\" \"npm run web-check-api-dev\"",
    "build": "npm run next-build && npm run fastapi-build && npm run web-check-api-build",
    "start": "concurrently \"npm run next-prod\" \"npm run fsqli-fastapi-prod\" \"npm run web-check-api-prod\"",
    "lint": "next lint"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@tabler/icons-react": "^3.10.0",
    "@types/node": "20.2.4",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "concurrently": "^8.0.1",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "lucide-react": "^0.407.0",
    "next": "13.4.4",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recharts": "^2.12.7",
    "tailwind-merge": "^2.4.0",
    "tailwindcss": "3.3.2",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.0.4"
  }
}
